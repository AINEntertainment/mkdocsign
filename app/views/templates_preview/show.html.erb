<template-builder
	class="grid"
	data-editable="false"
	data-with-sign-yourself-button="<%= !@template.archived_at? %>"
	data-with-send-button="<%= !@template.archived_at? && can?(:create, @template.submissions.new(account: current_account)) %>"
	data-template="<%= @template_data %>"
	data-with-phone="true"
	data-with-conditions="true"
	data-with-formula="true"
    data-with-verification="true"
	data-with-payment="false"
	data-with-logo="true"
></template-builder>
